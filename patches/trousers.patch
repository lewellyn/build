diff -ru trousers-0.3.8-orig/src/include/tss/tddlapi_error.h trousers-0.3.8/src/include/tss/tddlapi_error.h
--- trousers-0.3.8-orig/src/include/tss/tddlapi_error.h	2010-01-28 16:27:51.000000000 +0000
